{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "employee-actions-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/employeeActions/EmployeeActionsWebPart.js",
          "manifest": "./src/webparts/employeeActions/EmployeeActionsWebPart.manifest.json"
        }
      ]
    },
    "actions-progress-field-customizer": {
      "components": [
        {
          "entrypoint": "./lib/extensions/actionsProgress/ActionsProgressFieldCustomizer.js",
          "manifest": "./src/extensions/actionsProgress/ActionsProgressFieldCustomizer.manifest.json"
        }
      ]
    },
    "actions-notification-application-customizer": {
      "components": [
        {
          "entrypoint": "./lib/extensions/actionsNotification/ActionsNotificationApplicationCustomizer.js",
          "manifest": "./src/extensions/actionsNotification/ActionsNotificationApplicationCustomizer.manifest.json"
        }
      ]
    },
    "show-actions-command-set": {
      "components": [
        {
          "entrypoint": "./lib/extensions/showActions/ShowActionsCommandSet.js",
          "manifest": "./src/extensions/showActions/ShowActionsCommandSet.manifest.json"
        }
      ]
    }
  },
  "externals": {},
  "localizedResources": {
    "EmployeeActionsWebPartStrings": "lib/webparts/employeeActions/loc/{locale}.js",
    "ActionsProgressFieldCustomizerStrings": "lib/extensions/actionsProgress/loc/{locale}.js",
    "ActionsNotificationApplicationCustomizerStrings": "lib/extensions/actionsNotification/loc/{locale}.js",
    "ShowActionsCommandSetStrings": "lib/extensions/showActions/loc/{locale}.js"
  }
}
